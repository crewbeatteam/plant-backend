    "/v3/kb/plants/name_search": {
      "get": {
        "tags": [
          "Plant Knowledge Base"
        ],
        "summary": "Search plant knowledge base",
        "description": "Search plants by scientific names, common names, or synonyms. This endpoint is in beta testing.",
        "operationId": "get_PlantSearch",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "minLength": 1,
              "description": "Search query for plant names"
            },
            "required": true,
            "description": "Search query for plant names",
            "name": "q",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Maximum results (1-20, default 10)"
            },
            "required": false,
            "description": "Maximum results (1-20, default 10)",
            "name": "limit",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "default": "en",
              "description": "Language code for common names (ISO 639-1)"
            },
            "required": false,
            "description": "Language code for common names (ISO 639-1)",
            "name": "language",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Include 64x64 base64 thumbnails"
            },
            "required": false,
            "description": "Include 64x64 base64 thumbnails",
            "name": "thumbnails",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "API key for authentication"
            },
            "required": true,
            "description": "API key for authentication",
            "name": "Api-Key",
            "in": "header"
          }
        ],
        "responses": {
          "200": {
            "description": "Search results",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "entities": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "matched_in": {
                            "type": "string"
                          },
                          "matched_in_type": {
                            "type": "string",
                            "enum": [
                              "entity_name",
                              "synonym",
                              "common_name"
                            ]
                          },
                          "access_token": {
                            "type": "string"
                          },
                          "match_position": {
                            "type": "number"
                          },
                          "match_length": {
                            "type": "number"
                          },
                          "entity_name": {
                            "type": "string"
                          },
                          "thumbnail": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "matched_in",
                          "matched_in_type",
                          "access_token",
                          "match_position",
                          "match_length",
                          "entity_name"
                        ]
                      }
                    },
                    "entities_trimmed": {
                      "type": "boolean"
                    },
                    "limit": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "entities",
                    "entities_trimmed",
                    "limit"
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Invalid search parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "error"
                  ]
                }
              }
            }
          },
          "401": {
            "description": "Authentication failed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "error"
                  ]
                }
              }
            }
          }
        }
