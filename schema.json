  "paths": {
    "/v3/identification": {
      "post": {
        "tags": [
          "Plant Identification"
        ],
        "summary": "Identify plant species from images",
        "description": "Submit images for plant species identification using AI",
        "operationId": "post_PlantIdentification",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "description": "API key for authentication"
            },
            "required": true,
            "description": "API key for authentication",
            "name": "Api-Key",
            "in": "header"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "images": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "minItems": 1
                  },
                  "details": {
                    "type": "string"
                  },
                  "classification_level": {
                    "type": "string",
                    "enum": [
                      "all",
                      "genus",
                      "species",
                      "infraspecies"
                    ],
                    "default": "species"
                  },
                  "classification_raw": {
                    "type": "boolean",
                    "default": false
                  },
                  "language": {
                    "type": "string",
                    "default": "en"
                  },
                  "latitude": {
                    "type": "number"
                  },
                  "longitude": {
                    "type": "number"
                  },
                  "similar_images": {
                    "type": "boolean",
                    "default": false
                  },
                  "custom_id": {
                    "type": "number"
                  },
                  "datetime": {
                    "type": "string"
                  },
                  "health": {
                    "type": "string",
                    "enum": [
                      "only",
                      "auto",
                      "all"
                    ]
                  },
                  "suggestion_filter": {
                    "type": "object",
                    "properties": {
                      "classification": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "classification"
                    ]
                  },
                  "symptoms": {
                    "type": "boolean",
                    "default": false
                  }
                },
                "required": [
                  "images"
                ]
              }
            },
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "images": {
                    "type": "array",
                    "items": {},
                    "minItems": 1
                  },
                  "details": {
                    "type": "string"
                  },
                  "classification_level": {
                    "type": "string",
                    "enum": [
                      "all",
                      "genus",
                      "species",
                      "infraspecies"
                    ],
                    "default": "species"
                  },
                  "classification_raw": {
                    "type": "boolean",
                    "default": false
                  },
                  "language": {
                    "type": "string",
                    "default": "en"
                  },
                  "latitude": {
                    "type": "number"
                  },
                  "longitude": {
                    "type": "number"
                  },
                  "similar_images": {
                    "type": "boolean",
                    "default": false
                  },
                  "custom_id": {
                    "type": "number"
                  },
                  "datetime": {
                    "type": "string"
                  },
                  "health": {
                    "type": "string",
                    "enum": [
                      "only",
                      "auto",
                      "all"
                    ]
                  },
                  "suggestion_filter": {
                    "type": "object",
                    "properties": {
                      "classification": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "classification"
                    ]
                  },
                  "symptoms": {
                    "type": "boolean",
                    "default": false
                  }
                },
                "required": [
                  "images"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Plant identification completed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "access_token": {
                      "type": "string"
                    },
                    "custom_id": {
                      "type": "number"
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "is_plant": {
                          "type": "object",
